<nav>
  <ul class="breadcrumb">
    {% for item in nav.breadcrumb(url) %}
    {% if item.data and item.data.url != url %}
    <li class="breadcrumb-item"><a href="{{ item.data.url }}">{{ item.data.title }}</a></li>
    {% elif not item.data %}
    <li class="breadcrumb-item"><strong>{{ item.slug }}</strong></li>
    {% endif %}
    {% endfor %}
  </ul>
</nav>
